#include<stdio.h>
void selectionsort(int a[],int n)
{
    int i,j,temp;
    for(i=0;i<n-1;i++)
    {
    for(j=i+1;j<n;j++)
    {
    if(a[i]>a[j])
        {
        temp=a[i];
        a[i]=a[j];
        a[j]=temp;
        }
    }
    }
}
void printarray(int a[],int size)
{
    int i;
    for(i=0;i<size;i++)
    printf("%d ",a[i]);
}
    int main()
    {
    int a[20],i,n;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    selectionsort(a ,n);
    printf("sorted array ");
    printarray(a ,n);
}
